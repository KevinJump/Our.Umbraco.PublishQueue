<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata>
        <id>Our.Umbraco.PublishQueue</id>
        <version>1.1.0</version>
        <title>Umbraco Publication Queue</title>
        <authors>Kevin Jump,Darren Ferguson</authors>
        <owners>Kevin Jump</owners>
        <licenseUrl>http://www.opensource.org/licenses/mit-license.php</licenseUrl>
        <projectUrl>https://github.com/KevinJump/Our.Umbraco.PublishQueue/</projectUrl>
        <iconUrl>http://jumoo.co.uk/img/queueicon.png</iconUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Simple publication queue for Umbraco (7.5+)</description>
        <releaseNotes>
          0.0.1 - Intial release
          0.0.2 - Backwards Compatable SignalR install / Priority and scheduling on queue
          0.0.3 - custom data, and events
          0.0.5 - seperated out core (so you can put it in assemblies)
          0.0.6 - make the data column bigger
          0.0.9 - limit results to stop browser race
          1.0.0 - Paged queue results (for when your queue is massive) + background scheduling on 7.6
          1.0.1 - Removed SignalR.JS Dependency, and cleaned up Signal R Initialization
        </releaseNotes>
        <dependencies>
            <dependency id="Our.Umbraco.PublishQueue.Core" version="1.1.0" />
        </dependencies>
    </metadata>
  <files>
    <file src="..\Our.Umbraco.PublishQueue\App_Plugins\PublishQueue\**\*.*" target="content\App_Plugins\PublishQueue" />
  </files>
</package>